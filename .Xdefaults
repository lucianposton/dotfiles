Xcursor.theme:gentoo-silver

URxvt*.perl-ext-common: default,matcher
URxvt*.url-launcher: xdg-open
URxvt*.matcher.button:1
URxvt*.matcher.rend.0:Uline fg5
URxvt*.geometry: 120x35
URxvt*.depth: 32
URxvt*.background:  [80]#000000
URxvt*.foreground:  #dfdfdf
URxvt*.cursorColor: green
URxvt*.saveLines:10000
URxvt*.scrollBar:true
URxvt*.scrollstyle:plain
URxvt*.scrollBar_right:true
URxvt*.scrollTtyOutput:false
URxvt*.scrollWithBuffer:true
URxvt*.scrollTtyKeypress:true
!URxvt*.font: xft:Monospace:size=12
!URxvt*.font: xft:Monaco for Powerline:size=12
URxvt*.font: xft:Meslo LG S for Powerline:size=12

!URxvt*.colorBDMode:true
!URxvt*.colorBD: #FFFFFF

! Black + Bold DarkGrey
URxvt*.color0: #202020
URxvt*.color8:  #606060

! Red + Bold Red
URxvt*.color1: #cd0000
URxvt*.color9: #ff0000

! Green + Bold Green
URxvt*.color2: #00cd00
URxvt*.color10: #00ff00

! Yellow + Bold Yellow
URxvt*.color3: #cdcd00
URxvt*.color11: #ffff00

! Blue + Bold Blue
URxvt*.color4: #0050ee
URxvt*.color12: #0070ff

! Mangenta + Bold Mangenta
URxvt*.color5: #cd00cd
URxvt*.color13: #ff00ff

! Cyan + Bold Cyan
URxvt*.color6: #00cdcd
URxvt*.color14: #00cdff

! Grey + Bold White
URxvt*.color7: #909090
URxvt*.color15: #ffffff

