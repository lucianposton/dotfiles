Xcursor.theme:gentoo-silver

URxvt*.perl-ext-common: default,matcher,keyboard-select,selection-to-clipboard

URxvt*.keysym.Control-q: perl:matcher:last
URxvt*.keysym.Control-w: perl:matcher:list
URxvt*.url-launcher: xdg-open
URxvt*.matcher.button:1
URxvt*.matcher.pattern.0:(?:https?://|ftp://|news://|mailto:|file://)[-\\N{U+2013}\\w@;\\/?:&=%$.+!*\\x27,~#]+(?:\\([-\\N{U+2013}\\w@;\\/?:&=%$.+!*\\x27,~#]*\\)|[-\\N{U+2013}\\w@;\\/:&=%$+*~#])+
URxvt*.matcher.rend.0:Uline fg5

URxvt*.keysym.M-Escape: perl:keyboard-select:activate
URxvt*.keysym.M-s: perl:keyboard-select:search

! Emulate some xterm keycodes
URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.M-Up:          \033[1;3A
URxvt.keysym.M-Down:        \033[1;3B
URxvt.keysym.M-Left:        \033[1;3D
URxvt.keysym.M-Right:       \033[1;3C

URxvt*.geometry: 120x35
URxvt*.depth: 32
URxvt*.background:  [80]#000000
URxvt*.foreground:  #dfdfdf
URxvt*.cursorColor: green
URxvt*.saveLines:10000
URxvt*.scrollBar:true
URxvt*.scrollstyle:plain
URxvt*.thickness:8
URxvt*.scrollBar_right:true
URxvt*.scrollTtyOutput:false
URxvt*.scrollWithBuffer:true
URxvt*.scrollTtyKeypress:true
!URxvt*.font: xft:Monospace:size=12
!URxvt*.font: xft:Monaco for Powerline:size=12
URxvt*.font: xft:Meslo LG S for Powerline:size=12

!URxvt.colorIT:      #87af5f
!URxvt.colorBD:      #0050ee
!URxvt.colorUL:      #87afd7

!URxvt*.colorBDMode:true
!URxvt*.colorBD: #FFFFFF

! Black + Bold DarkGrey
URxvt*.color0: #202020
URxvt*.color8:  #606060

! Red + Bold Red
URxvt*.color1: #cd0000
URxvt*.color9: #ff0000

! Green + Bold Green
URxvt*.color2: #00cd00
URxvt*.color10: #00ff00

! Yellow + Bold Yellow
URxvt*.color3: #cdcd00
URxvt*.color11: #ffff00

! Blue + Bold Blue
URxvt*.color4: #0050ee
URxvt*.color12: #0070ff

! Mangenta + Bold Mangenta
URxvt*.color5: #cd00cd
URxvt*.color13: #ff00ff

! Cyan + Bold Cyan
URxvt*.color6: #00cdcd
URxvt*.color14: #00cdff

! Grey + Bold White
URxvt*.color7: #909090
URxvt*.color15: #ffffff

